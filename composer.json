{
    "name": "jesperbeisner/fwstats",
    "description": "Statistics website for the world 'ActionFreewar' in the browser game freewar.de",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jesper Beisner",
            "email": "jesper.beisner+github@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "nikic/fast-route": "^1.3",
        "psr/container": "^2.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.9",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "csfixer": "vendor/bin/php-cs-fixer fix --diff",
        "phpunit": "vendor/bin/phpunit --do-not-cache-result",
        "phpstan": "vendor/bin/phpstan",
        "test": {
            "csfixer": "@csfixer",
            "phpunit": "@phpunit",
            "phpstan": "@phpstan"
        }
    },
    "autoload": {
        "psr-4": {
            "Jesperbeisner\\Fwstats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jesperbeisner\\Fwstats\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
